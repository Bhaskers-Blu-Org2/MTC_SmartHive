<forms:WindowsPage
    x:Class="SmartHive.LevelMapApp.UWP.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:forms="using:Xamarin.Forms.Platform.UWP"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:SmartHive.LevelMapApp.UWP"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <WebView HorizontalAlignment="Left" Source="http://cloud.wiregeo.com/?PHJXfrNo&amp;frame" Margin="10,10,0,0" 
            VerticalAlignment="Top" Width="1200" Height="720" x:Name="floorMapView" ScriptNotify="floorMapView_ScriptNotify"/>
    </Grid>
</forms:WindowsPage>
